node('ubuntu-spc') 
{
    env.PATH="/usr/lib/jvm/java-8-openjdk-amd64/bin:$PATH"
    stage('clone') 
    {
        git credentialsId: 'Ubuntu-Node', 
            url: 'https://github.com/wakaleo/game-of-life.git',
            branch: 'master'
    }
    stage('build') 
    {
        sh 'mvn package'
    }
}