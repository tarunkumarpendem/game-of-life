node('ubuntu-spc')
{
    stage('java_path')
    {
    tool name: 'JDK8', type: 'jdk'
    }
    stage('maven_path')
    {
    tool name: 'Maven 3.6.3', type: 'maven'
   }
   stage('clone') 
   {
    git credentialsId: 'Ubuntu-Node', 
        url: 'https://github.com/tarunkumarpendem/game-of-life.git'
   }
   stage('package') 
   {
    sh '/usr/share/maven/bin/mvn package'
   }
}

